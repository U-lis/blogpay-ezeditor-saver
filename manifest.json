{
  "name": "Blogpay-EZeditor-Saver",
  "description": "Lifesaver of EZeditor users",
  "version": "0.1",
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": ["script.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://morethanmoon.shop.blogpay.co.kr/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_icon": {
      "16": "/icon/icon_16.png",
      "32": "/icon/icon_32.png"
    }
  },
  "icons": {
    "16": "/icon/icon_16.png",
    "32": "/icon/icon_32.png"
  }
}
